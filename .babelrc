{
  "stage": 0,
  "optional": ["runtime"],
  "plugins": ["uglify:after"],
  "env": {
    "development": {
      "extra": {
        "react-transform": [{
          "target": "react-transform-webpack-hmr",
          "imports": ["react"],
          "locals": ["module"]
        }, {
          "target": "react-transform-catch-errors",
          "imports": ["react", "redbox-react"]
        }]
      }
    }
  }
}
